# Environment Variables for VibeScape
# Copy this file to .env and fill in your actual values

# MongoDB Atlas Connection String
MONGODB_URI=mongodb+srv://vibescapeUser:YOUR_PASSWORD@cluster0.dfq4mbe.mongodb.net/vibescape?retryWrites=true&w=majority

# Server Port (Render will provide this automatically)
PORT=3030

# JWT Secret (for authentication - generate a random string)
JWT_SECRET=your_jwt_secret_key_here

# Node Environment
NODE_ENV=production